<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.1"
                               name="IsyFact">
    <description>Stellt Regeln zur Verwendung der IsyFact bereit.</description>
    <rules>
        <resource>batch.xml</resource>
        <resource>core.xml</resource>
        <resource>datetime.xml</resource>
        <resource>fehlerbehandlung.xml</resource>
        <resource>gui.xml</resource>
        <resource>konfiguration.xml</resource>
        <resource>layer.xml</resource>
        <resource>logging.xml</resource>
        <resource>maven.xml</resource>
        <resource>package.xml</resource>
        <resource>persistenz.xml</resource>
        <resource>service.xml</resource>
        <resource>spring.xml</resource>
        <resource>ueberwachung.xml</resource>
        <resource>isyfact-groups.xml</resource>
    </rules>
</jqa-plugin:jqassistant-plugin>
