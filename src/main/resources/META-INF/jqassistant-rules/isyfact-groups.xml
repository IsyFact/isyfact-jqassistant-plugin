<jqassistant-rules
        xmlns="http://schema.jqassistant.org/rule/v1.8"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://schema.jqassistant.org/rule/v1.8 https://schema.jqassistant.org/rule/jqassistant-rule-v1.8.xsd">

    <group id="isyfact:Verbindlich">
        <includeGroup refId="isyfact:batch:Verbindlich"/>
        <includeGroup refId="isyfact:core:Verbindlich"/>
        <includeGroup refId="isyfact:datetime:Verbindlich"/>
        <includeGroup refId="isyfact:konfiguration:Verbindlich"/>
        <includeGroup refId="isyfact:layer:Verbindlich"/>
        <includeGroup refId="isyfact:logging:Verbindlich"/>
        <includeGroup refId="isyfact:package:Verbindlich"/>
        <includeGroup refId="isyfact:service:Verbindlich"/>
    </group>

    <group id="isyfact:Empfohlen">
        <includeGroup refId="isyfact:batch:Empfohlen"/>
        <includeGroup refId="isyfact:core:Empfohlen"/>
        <includeGroup refId="isyfact:fehlerbehandlung:Empfohlen"/>
        <includeGroup refId="isyfact:gui:Empfohlen"/>
        <includeGroup refId="isyfact:konfiguration:Empfohlen"/>
        <includeGroup refId="isyfact:persistence:Empfohlen"/>
        <includeGroup refId="isyfact:service:Empfohlen"/>
        <includeGroup refId="isyfact:ueberwachung:Empfohlen"/>
    </group>

</jqassistant-rules>